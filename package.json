        "usetube": "^2.2.7",
        "uuid": "^9.0.1",
        "youtube-transcript": "^1.0.6"
    },
    "devDependencies": {
        "@pinecone-database/pinecone": "^2.0.1",
        "@qdrant/js-client-rest": "^1.7.0",
        "@tsconfig/recommended": "^1.0.3",
        "@types/debug": "^4.1.12",
        "@types/html-to-text": "^9.0.4",
        "@types/md5": "^2.3.5",
        "@types/node": "^20.11.14",
        "@types/usetube": "^2.1.2",
